
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path '10180000.wmac'
	option htmode 'HT20'
	option country 'CN'
	option channel '13'
	option txpower '20'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'PassGFW'
	option encryption 'none'

